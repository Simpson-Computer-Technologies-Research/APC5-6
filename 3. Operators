//////////////////////////
//      Operators       //
//////////////////////////


// Variable 1 is equal to Variable2
var var_1 = 1
var var_2 = 1


// Variable 1 is equal to Variable2
// 
//     ==
// 
// var_3 = var_1 == var_2  var_3 = True




// Variable 1 is not equal to Variable2
// 
//     !=
// 
// var_4 = var_1 != var_2  var_4 = False



// Variable 1 is greater than Variable2
//
//    >
//
// var_3 = var_1 > var_2  var_3 = True



// Variable 1 is less than Variable2
// 
//     <
// 
// var_3 = var_1 < var_2  var_3 = True



// Variable 1 is greater than or equal to Variable2
// 
//     >=
// 
// var_5 = var_1 >= var_2  var_3 = True



// Variable 1 is less than or equal to Variable2
// 
//     <=
// 
// var_6 = var_1 <= var_2  var_6 = True
